{
  "name": "js-html-game",
  "version": "0.1.0",
  "description": "html games",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "run-z test:vitest test:wasm",
    "test:vitest": "vitest run",
    "test:wasm": "cargo test",
    "lint": "run-z lint:format lint:eslint lint:biome lint:typescript lint:format-wasm lint:wasm",
    "lint:format": "biome ci .",
    "lint:eslint": "eslint src --cache",
    "lint:biome": "biome check .",
    "lint:typescript": "tsc",
    "lint:format-wasm": "cargo fmt --check",
    "lint:wasm": "cargo clippy",
    "lint-fix:eslint": "eslint src --cache --fix",
    "lint-fix:biome": "biome check . --apply",
    "format:biome": "biome format . --write",
    "format:wasm": "cargo fmt",
    "build": "run-z build-wasm build-js:astro",
    "build-js:astro": "astro build",
    "build-wasm": "run-z build-wasm:chess",
    "build-wasm:chess": "wasm-pack build src/wasms/chess --release",
    "check-all:js": "run-z format:biome lint:biome lint:eslint lint:typescript test:vitest build-js:astro",
    "check-all:wasm": "run-z format:wasm build-wasm lint:wasm test:wasm",
    "check-all": "run-z check-all:wasm check-all:js",
    "ci": "run-z ci:wasm ci:js",
    "ci:js": "run-z lint:format lint:eslint lint:typescript test:vitest build-js:astro",
    "ci:wasm": "run-z lint:format-wasm lint:wasm test:wasm build-wasm",
    "serve": "astro dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikiymk/js-html-game.git"
  },
  "author": "mikiymk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikiymk/js-html-game/issues"
  },
  "homepage": "https://github.com/mikiymk/js-html-game#readme",
  "devDependencies": {
    "@biomejs/biome": "^1.2.2",
    "@eslint/js": "^8.50.0",
    "@tsconfig/strictest": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-i": "^2.28.1",
    "eslint-plugin-solid": "^0.13.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "eslint-plugin-vitest": "^0.3.2",
    "globals": "^13.22.0",
    "jsdom": "^22.1.0",
    "run-z": "^2.0.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.10",
    "vite-plugin-solid": "^2.7.0",
    "vite-plugin-top-level-await": "^1.3.1",
    "vite-plugin-wasm": "^3.2.2",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "@astrojs/solid-js": "^3.0.1",
    "astro": "^3.2.2",
    "solid-js": "^1.7.12"
  }
}