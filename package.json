{
  "name": "browser-games",
  "version": "0.1.0",
  "description": "html games",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "run-z test:vitest test:zig",
    "test:vitest": "vitest run",
    "test:wasm": "zig build test",
    "lint": "run-z lint:biome lint:eslint lint:typescript lint:knip",
    "lint:biome": "biome ci .",
    "lint:biome:fix": "biome check . --apply",
    "lint:eslint": "eslint src --cache",
    "lint:eslint:fix": "eslint src --cache --fix",
    "lint:eslint-timing": "export TIMING=1 || set TIMING=1 && eslint src",
    "lint:typescript": "tsc",
    "lint:knip": "knip --cache",
    "lint:format-zig": "zig fmt --check ./src",
    "format": "run-z format:biome format:wasm",
    "format:biome": "biome format . --write",
    "format:wasm": "zig fmt ./src",
    "build": "run-z build:wasm:fast build:js",
    "build:js": "astro build",
    "build:wasm": "zig build all -Dtarget=wasm32-freestanding -Doptimize=Debug",
    "build:wasm:fast": "zig build all -Dtarget=wasm32-freestanding -Doptimize=ReleaseFast",
    "build:wasm:small": "zig build all -Dtarget=wasm32-freestanding -Doptimize=ReleaseSmall",
    "check:js": "run-z format:biome lint:biome lint:eslint lint:typescript lint:knip build:js",
    "check:wasm": "run-z format:wasm test:wasm build:wasm:fast",
    "check": "run-z check:wasm check:js",
    "ci": "run-z ci:wasm ci:js",
    "ci:js": "run-z lint:format lint:eslint lint:typescript lint:knip build:js",
    "ci:wasm": "run-z lint:format-zig test:wasm build:wasm:fast",
    "serve": "astro dev",
    "serve:preview": "astro preview"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikiymk/.git"
  },
  "author": "mikiymk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikiymk/browser-games/issues"
  },
  "homepage": "https://github.com/mikiymk/browser-games#readme",
  "devDependencies": {
    "@astrojs/solid-js": "^4.3.0",
    "@astrojs/tailwind": "^5.1.0",
    "@biomejs/biome": "^1.8.1",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.3.0",
    "@eslint/js": "^9.5.0",
    "@tsconfig/strictest": "^2.0.5",
    "astro": "^4.10.2",
    "astro-compress": "^2.2.28",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-i": "^2.29.1",
    "eslint-plugin-solid": "^0.14.0",
    "eslint-plugin-unicorn": "^54.0.0",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^15.4.0",
    "jsdom": "^24.1.0",
    "knip": "^5.19.0",
    "run-z": "^2.0.0",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.13.0",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "solid-js": "^1.8.17"
  }
}
