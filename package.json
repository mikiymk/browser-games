{
  "name": "js-html-game",
  "version": "0.1.0",
  "description": "html games",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "run-s test:*",
    "test:vitest": "vitest run",
    "test:wasm": "cargo test",
    "lint:format": "biome ci .",
    "lint:eslint": "eslint **/*.ts **/*.tsx --cache",
    "lint:biome": "biome lint .",
    "lint:typescript": "tsc",
    "lint:format-wasm": "cargo fmt --check",
    "lint:wasm": "cargo clippy",
    "lint-fix:eslint": "eslint **/*.ts **/*.tsx --cache --fix",
    "lint-fix:biome": "biome lint . --apply",
    "format:biome": "biome format . --write",
    "format:wasm": "cargo fmt",
    "build": "run-s build-wasm:* build-js:vite",
    "build-js:vite": "vite build",
    "build-wasm": "run-s build-wasm:*",
    "build-wasm:chess": "wasm-pack build chess/wasm --release",
    "check-all:js": "run-s format:biome lint:biome lint:eslint lint:typescript test:vitest build-js:vite",
    "check-all:wasm": "run-s format:wasm build-wasm:* lint:wasm test:wasm",
    "check-all": "run-s check-all:wasm check-all:js",
    "ci": "npm-run-all -p lint:format lint:format-wasm -s build-wasm:* -p lint:* test:* -s build",
    "serve": "vite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikiymk/js-html-game.git"
  },
  "author": "mikiymk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikiymk/js-html-game/issues"
  },
  "homepage": "https://github.com/mikiymk/js-html-game#readme",
  "devDependencies": {
    "@biomejs/biome": "^1.2.2",
    "@eslint/js": "^8.50.0",
    "@tsconfig/strictest": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-solid": "^0.13.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "eslint-plugin-vitest": "^0.3.2",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.2.2",
    "vite": "^4.4.10",
    "vite-plugin-solid": "^2.7.0",
    "vite-plugin-top-level-await": "^1.3.1",
    "vite-plugin-wasm": "^3.2.2",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "solid-js": "^1.7.12"
  }
}